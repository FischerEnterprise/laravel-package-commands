{
    "name": "fischer-enterprise/laravel-package-commands",
    "description": "A new package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Fischer",
            "email": "ben@fischer-enterprise.de"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "autoload": {
        "psr-4": {
            "FischerEnterprise\\LaravelPackageCommands\\": "src",
            "FischerEnterprise\\LaravelPackageCommands\\Presets": "presets"
        }
    },
    "autoload_dev": {
        "psr-4": {
            "FischerEnterprise\\LaravelPackageCommands\\Tests\\": "tests"
        }
    },
    "require_dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "sh vendor/bin/phpunit",
        "test-f": "sh vendor/bin/phpunit --filter"
    }
}
